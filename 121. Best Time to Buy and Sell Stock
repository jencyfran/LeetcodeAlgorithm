class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        if not prices:
            return 0
        sell = 0
        buy = prices[0]
        for i in range(1, len(prices)):
            sell = max(sell, prices[i] - buy)
            buy = min(buy, prices[i])
        return sell
        
        
        
        
        
        
        
        
        
        
